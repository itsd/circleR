<section class="circles-section">

    <div id="circle1" class="circle-root">
        <svg>
            <circle class="cover-circle" />
            <path class="circle-main" />
        </svg>
        <div class="content">
            <div class="percent">0</div>
        </div>
    </div>

    <div id="circle2" class="circle-root">
        <svg>
            <circle class="cover-circle" />
            <path class="circle-main" />
        </svg>
        <div class="content">
            <div class="percent">0</div>
        </div>
    </div>

    <div id="circle3" class="circle-root">
        <svg>
            <circle class="cover-circle" />
            <path class="circle-main" />
        </svg>
        <div class="content">
            <div class="percent">0</div>
        </div>
    </div>

    <div id="circle4" class="circle-root">
        <svg>
            <circle class="cover-circle" />
            <path class="circle-main" />
        </svg>
        <div class="content">
            <div class="percent">0</div>
        </div>
    </div>

</section>

@section scripts{
    <script type="text/javascript">
        $(function () {
            setTimeout(function () {
                $('#circle1').circler({ fillPercent: 92 }).animate();
                $('#circle2').circler({ fillPercent: 85 }).animate();
                $('#circle3').circler({ fillPercent: 90 }).animate();
                $('#circle4').circler({ fillPercent: 97 }).animate();
            }, 300);
        });
    </script>
}